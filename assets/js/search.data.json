---
layout: false
---
{% capture newline %}
{% endcapture %}
[
  {% for page in site.pages %}
    {
      "title": "{{ page.title | strip_html | url_encode }}",
      "url": "{{ site.baseurl }}{{ page.url }}",
      "content": "{{ page.content | strip_html | replace: newline, " "  | downcase | url_encode }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
